// fungsi untuk mengambil nama bulan tertentu
function getBulan($bulan) {
  $arrbulan = array (1 => 'January', 'February', 'March', 'April',
          'May', 'June', 'July', 'August', 'September',
          'October', 'November', 'December');
  return $arrbulan[$bulan];
}

// fungsi untuk mengambil nama bulan tertentu, dengan nama singkat
function getBulanSingkat($bulan) {
  $arrbulan = array (1 => 'Jan', 'Feb', 'Mar', 'Apr',
          'May', 'Jun', 'Jul', 'Aug', 'Sep',
          'Oct', 'Nov', 'Dec');
  return $arrbulan[$bulan];
}

// fungsi untuk mengambil nama hari tertentu
function getNamaHari($hari) {
  $arrHari = array (0 => 'Sunday', 'Monday', 'Tuesday', 'Wednesday',
          'Thusday', 'Friday', 'Saturday', 'Sunday');
  return $arrHari[$hari];
}
function getNamaHariSingkat($hari) 
{
  if (is_numeric($hari))
  {
    $arrHari = array (0 => 'Sun', 'Mon', 'Tue', 'Wed',
            'Thu', 'Fri', 'Sat', 'Sun');
    return $arrHari[$hari];
  }
  else return "";

}
